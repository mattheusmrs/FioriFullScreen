<mvc:View controllerName="student11.com.sap.training.ux402.fullscreen.fullscreen.controller.Carrier"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>overviewPageTitle}">
        <content>
            <Table id="carrierTable" items="{/UX_C_Carrier_TP}">
                <columns>
                    <Column id="column1" minScreenWidth="Tablet" demandPopin="true">
                        <Text id="text1" text="{i18n>columnId}" />
                    </Column>
                    <Column id="column2" minScreenWidth="Tablet" demandPopin="true">
                        <Text id="text2" text="{i18n>columnName}" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem id="colimnListItem1" press="onPress" type="Navigation">
                        <cells>
                            <ObjectIdentifier id="objectIdentifier1" text="{Carrid}" />
                            <Text id="text3" text="{Carrname}"></Text>
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
    </Page>
</mvc:View>
